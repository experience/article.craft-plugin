{
  "name": "experience/article",
  "description": "Article eases the pain of rendering Matrix and Markdown-powered content.",
  "type": "craft-plugin",
  "version": "2.0.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "markdown",
    "typography"
  ],
  "support": {
    "docs": "https://github.com/experience/article.craft-plugin/blob/master/README.md",
    "issues": "https://github.com/experience/article.craft-plugin/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Experience",
      "homepage": "https://experiencehq.net"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0",
    "experience/smartdown": "^3.0.1"
  },
  "autoload": {
    "psr-4": {
      "experience\\article\\": "src/"
    }
  },
  "extra": {
    "changelogUrl": "https://github.com/experience/article.craft-plugin/blob/master/CHANGELOG.md",
    "class": "experience\\article\\Article",
    "handle": "article",
    "hasCpSection": false,
    "hasSettings": true,
    "name": "Article",
    "sourceLanguage": "en-GB"
  }
}
